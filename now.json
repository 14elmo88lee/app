{
  "name": "wip-beta",
  "alias": "wip-beta",
  "scale": {
    "all": {
      "min": 1,
      "max": 2
    }
  },
  "env": {
    "APP_ID": "@beta-app-id",
    "APP_NAME": "WIP (beta)",
    "DISABLE_STATS": "true",
    "LOG_LEVEL": "@beta-log-level",
    "LOGDNA_API_KEY": "@beta-logdna-api-key",
    "NODE_ENV": "production",
    "PRIVATE_KEY": "@beta-private-key",
    "REDIS_URL": "@beta-redis-url",
    "SENTRY_DSN": "@beta-sentry-dsn",
    "WEBHOOK_SECRET": "@beta-webhook-secret"
  }
}
